Algoritmo "dobrodonumero"
//entrada:digitar o lado
//processamento:calcular area dos lados do terreno
//saida: motrar valor do terreno

Var
   lado:real
   area:real

Inicio
   escreva("digite o valor de um lado do terreno ")
   leia(lado)
   
   area <- lado*lado
   
   escreval("o valor do  do terreno é ",area)


Fimalgoritmo